<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
  "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <display-name>Controladores de Cadastro</display-name>

  <enterprise-beans>
    <session>
      <display-name>ControladorAtualizacaoCadastroGCOM</display-name>
      <ejb-name>ControladorAtualizacaoCadastroGCOM</ejb-name>
      <local-home>gcom.cadastro.atualizacaocadastral.ControladorAtualizacaoCadastroLocalHome</local-home>
      <local>gcom.cadastro.atualizacaocadastral.ControladorAtualizacaoCadastroLocal</local>
      <ejb-class>gcom.cadastro.atualizacaocadastral.ControladorAtualizacaoCadastro</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>

  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>ControladorAtualizacaoCadastroGCOM</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>

  <ejb-client-jar>ControladorAtualizacaoCadastroGCOM.jar</ejb-client-jar>
</ejb-jar>
